{
	"name": "jar-viewer",
	"displayName": "JAR Viewer",
	"description": "Lists classes and files inside JAR archives.",
	"version": "1.1.1",
	"publisher": "wmanth",
	"author": {
		"name": "Wolfram Manthey"
	},
	"license": "MIT",
	"repository": {
		"url": "https://github.com/wmanth/vscode-jar-viewer.git"
	},
	"bugs": {
		"url": "https://github.com/wmanth/vscode-jar-viewer/issues"
	},
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"java",
		"jar"
	],
	"activationEvents": [
		"onCustomEditor:jar.contentView"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "jar.contentView",
				"displayName": "Jar Content",
				"selector": [
					{
						"filenamePattern": "*.jar"
					},
					{
						"filenamePattern": "*.srcjar"
					},
					{
						"filenamePattern": "*.aar"
					},
					{
						"filenamePattern": "*.war"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "npm-run-all compile:*",
		"compile:extension": "tsc -p ./",
		"compile:app": "webpack --mode development",
		"watch": "npm-run-all -p watch:*",
		"watch:extension": "tsc -watch -p ./",
		"watch:app": "webpack --watch --mode development",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chai": "^4.2.18",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/react": "^17.0.5",
		"@types/react-dom": "^17.0.4",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.28.2",
		"@typescript-eslint/parser": "^4.28.2",
		"chai": "^4.3.4",
		"css-loader": "^5.2.4",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"npm-run-all": "^4.1.5",
		"style-loader": "^2.0.0",
		"ts-loader": "^9.1.2",
		"ts-node": "^10.0.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.0",
		"webpack": "^5.37.0",
		"webpack-cli": "^4.7.0"
	},
	"dependencies": {
		"jszip": "^3.6.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-icons": "^4.2.0"
	}
}
