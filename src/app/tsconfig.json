{
	"compilerOptions": {
		"module": "esnext",
		"moduleResolution": "node",
		"target": "es6",
		"outDir": "jarViewer",
		"lib": [
			"es6",
			"dom"
		],
		"jsx": "react-jsx",
		"sourceMap": true,
		"rootDir": "..",
		"noUnusedLocals": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"experimentalDecorators": true
	},
	"exclude": [
		"node_modules"
	]
}
